dat <- read_csv("Data/hotel_bookings.csv")
library(tidyverse)
library(readr)
library(kableExtra)
library(ggplot2)
library(lubridate)
library(gridExtra)
library(rmdformats)
dat <- read_csv("Data/hotel_bookings.csv")
# process the data to only contains information of city hotel
dat2 <- dat %>% dplyr::filter(hotel %in% c("City Hotel")
)
dat3 <-dat2 %>%
dplyr::select(arrival_date_month) %>%
group_by(arrival_date_month) %>%
mutate(arrivals = n()) %>%
unique() %>%
arrange(-arrivals)
#dat3 <-dat2 %>%
# count(arrival_date_month, names = "arrivals", sort = TRUE)
kable(dat3, caption = "Number of Arrivals per month in 2016") %>%
kable_styling(bootstrap_options = c("striped", "hover"))
